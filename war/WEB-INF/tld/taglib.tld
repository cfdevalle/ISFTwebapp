    <body-content>empty</body-content>
    <description>Tag with Parameter</description>
    <attribute>
      <name>nombreCombo</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <type>String</type>
    </attribute>
    <attribute>
